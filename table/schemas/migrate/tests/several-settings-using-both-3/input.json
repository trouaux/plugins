{
  "datasource": {
    "type": "prometheus",
    "uid": "${DS_PROM}"
  },
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "thresholds"
      },
      "custom": {
        "align": "auto",
        "cellOptions": {
          "type": "auto"
        },
        "inspect": false
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 80
          }
        ]
      }
    },
    "overrides": [
      {
        "matcher": {
          "id": "byName",
          "options": "API server"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 300
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "endpoint"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 75
          }
        ]
      }
    ]
  },
  "gridPos": {
    "h": 8,
    "w": 12,
    "x": 12,
    "y": 8
  },
  "id": 14,
  "options": {
    "cellHeight": "sm",
    "footer": {
      "countRows": false,
      "fields": "",
      "reducer": [
        "sum"
      ],
      "show": false
    },
    "showHeader": true
  },
  "pluginVersion": "10.1.8",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "editorMode": "code",
      "exemplar": false,
      "expr": "up{stack=\"ccp-ne-ogob01a\", prometheus=\"platform\"}",
      "format": "table",
      "instant": true,
      "legendFormat": "__auto",
      "range": false,
      "refId": "UP"
    }
  ],
  "title": "UP status",
  "transformations": [
    {
      "disabled": true,
      "id": "seriesToRows",
      "options": {}
    },
    {
      "id": "labelsToFields",
      "options": {}
    },
    {
      "id": "organize",
      "options": {
        "excludeByName": {
          "Time": true,
          "__name__": true
        },
        "indexByName": {
          "__name__": 1,
          "Time": 0,
          "Value": 3,
          "apiserver": 2
        },
        "renameByName": {
          "Time": "",
          "Value": "ValueCustomHeader",
          "apiserver": "API server"
        }
      }
    }
  ],
  "type": "table"
}