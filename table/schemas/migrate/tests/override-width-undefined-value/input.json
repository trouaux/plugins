{
  "datasource": {
    "type": "prometheus",
    "uid": "${DATASOURCE}"
  },
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "thresholds"
      },
      "custom": {
        "align": "auto",
        "cellOptions": {
          "type": "auto"
        },
        "inspect": false
      },
      "mappings": []
    },
    "overrides": [
      {
        "matcher": {
          "id": "byName",
          "options": "Reason"
        },
        "properties": [
          {
            "id": "custom.width"
          }
        ]
      }
    ]
  },
  "gridPos": {
    "h": 8,
    "w": 12,
    "x": 0,
    "y": 0
  },
  "id": 1,
  "options": {
    "cellHeight": "sm",
    "footer": {
      "countRows": false,
      "fields": "",
      "reducer": [
        "sum"
      ],
      "show": false
    },
    "showHeader": true
  },
  "pluginVersion": "10.1.8",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DATASOURCE}"
      },
      "editorMode": "code",
      "exemplar": false,
      "expr": "sum by (job) (up{$filter})",
      "format": "table",
      "instant": true,
      "legendFormat": "__auto",
      "range": false,
      "refId": "A"
    }
  ],
  "title": "Override width but doesnt define a value",
  "description": "When there is a width override that doesn't define a value, it falls back to auto => we should set auto explicitly in Perses",
  "type": "table"
}