{
  "kind": "PrometheusTimeSeriesQuery",
  "spec": {
    "datasource": {
      "kind": "PrometheusDatasource",
      "name": "${datasource}"
    },
    "minStep": "",
    "query": "avg without (instance) (hadoop_hdfs_datanode_capacity{bar_stack_id=\"$bar_stack_id\", _target=~\"$datanode\"})",
    "seriesNameFormat": "{{_target}}"
  }
}